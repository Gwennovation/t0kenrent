{
  "babbage": {
    "name": "T0kenRent",
    "version": "1.0.0",
    "description": "Decentralized Rental Tokenization and Escrow Platform - Tokenize and rent everyday assets on BSV blockchain with HTTP 402 payment gating",
    "author": "ChibiTech",
    "icon": "/t0kenrent-logo.png",
    "groupPermissions": {
      "protocolPermissions": [
        {
          "protocolID": [2, "Pay MNEE"],
          "counterparty": "self",
          "description": "Create receive addresses for rental payments and deposits"
        },
        {
          "protocolID": [2, "HTTP 402"],
          "counterparty": "anyone",
          "description": "Process HTTP 402 micropayments to unlock rental details"
        },
        {
          "protocolID": [1, "messagebox"],
          "counterparty": "self",
          "description": "Decrypt inbound rental notifications and escrow updates"
        },
        {
          "protocolID": [1, "messagebox advertisement"],
          "counterparty": "anyone",
          "description": "Advertise rental asset availability"
        },
        {
          "protocolID": ["tm_tokenrent"],
          "counterparty": "anyone",
          "description": "Record rental transactions on T0kenRent overlay network"
        },
        {
          "protocolID": ["BRC-76"],
          "counterparty": "anyone",
          "description": "Mint and transfer BRC-76 compliant rental asset tokens"
        }
      ],
      "basketAccess": [
        {
          "basket": "MNEE tokens",
          "description": "For rental payments and security deposits"
        },
        {
          "basket": "Rental Asset Tokens",
          "description": "For BRC-76 tokens representing rentable assets"
        },
        {
          "basket": "HTTP 402 Payments",
          "description": "For micropayments to unlock rental details"
        },
        {
          "basket": "Rental Escrows",
          "description": "For security deposit escrow transactions"
        }
      ],
      "certificateAccess": [
        {
          "type": "identity",
          "fields": ["Name", "Email", "Phone"],
          "verifierPublicKey": "...",
          "description": "For identity verification of rental participants"
        }
      ]
    }
  },
  "name": "T0kenRent",
  "short_name": "T0kenRent",
  "description": "Decentralized Rental Tokenization Platform",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#2563eb",
  "icons": [
    {
      "src": "/t0kenrent logo.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/t0kenrent logo.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ]
}
